// EXPECTED_RESULT: mux (($CYCLE - 1) % 6 < 3) { ($CYCLE - 1) % 3 + 1 } else { -(($CYCLE - 1) % 3) - 1 }
import java.lang.Math.cos
import java.lang.Math.PI
def cycle(start, step) {
	cos(rep(x <- start) {x + step})
}
def switch() {
	if(cycle(-PI*2/3, PI/3) >= 0) {
		() -> {
			rep(x <- 0) {
				x + 1
			}
		}
	} else {
		() -> {
			rep(x <- 0) {
				x - 1
			}
		}
	}
}
((switch).apply()).apply()
