/*
 * Test to make sure that more than one instance of a function can be active at a time
 * f -> g -> f -> h -> f 
 * 
 * EXPECTED_RESULT: 6
 */ 
def f(x,y){
	if(x) {
		2+g();
	} else {
	    if(y) {
	    	1+h();
	    } else {
			3;
		}
	}
}

def g() {
  f(false,true);
}

def h() {
  f(false,false);
}

f(true,false);